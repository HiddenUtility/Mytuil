{
	"main文": {
		"prefix": "main",
		"body": [
			"class ${1:Main}:",
			"    def __init__(self):...",
			"",
			"    @staticmethod",
			"    def main():",
			"        ${2:...}",
			"",
			"if __name__ == '__main__':",
			"    Main.main()"
		]
		
	},


	"test文": {
		"prefix": "test",
		"body": [
			"class ${1:Test}:",
			"    def __init__(self):...",
			"",
			"    def ${2:test_mehotd0}(self):",
			"        ...",
			"",
			"    def ${3:test_mehotd1}(self):",
			"        ...",
			"",

			"if __name__ == '__main__':",
			"    test = $1()",
			"    test.$2()",
			"    test.$3()"

		]
		
	},


	"インタフェース": {
		"prefix": "interface",
		"body": [
			"from abc import ABC, abstractmethod",
			"",
			"",
			"class Interface(ABC):",
			"    ${1:...}"
		]
	},

	"アノテーション": {
		"prefix": "annotations",
		"body": [
			"from __future__ import annotations\n"
		]
	},

